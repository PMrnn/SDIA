Object subclass: #List	instanceVariableNames: 'begin end size'	classVariableNames: ''	package: 'TP4'!!List methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 14:53'!size: anObject	size := anObject! !!List methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 14:32'!isEmpty 	^ begin isNil! !!List methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 14:41'!size	^ size		! !!List methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 15:02'!initialize	super initialize.	size := 0.	begin:= Node new.	end:= Node new! !!List methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 13:43'!end 	^ end! !!List methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 15:10'!at: aNumber	| node |	node := begin.	1 to: aNumber do: [ :i | node := node nextNode ].	^ node value! !!List methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 14:53'!begin	^ begin! !!List methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 15:01'!add: aValue	| node endNode |	node := Node new.	node value: aValue.	self isEmpty		ifTrue: [ begin := node ]		ifFalse: [ endNode := end.			node prevNode: end.			endNode nextNode: node ].	size := size + 1! !!List methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 13:43'!end: anObject	end:= anObject! !!List methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 13:43'!begin: anObject	begin:= anObject! !Object subclass: #Node	instanceVariableNames: 'value nextNode prevNode'	classVariableNames: ''	package: 'TP4'!!Node methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 13:49'!prevNode: anObject	prevNode:= anObject! !!Node methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 13:48'!nextNode	^ nextNode! !!Node methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 13:49'!nextNode: anObject	nextNode:= anObject! !!Node methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 13:48'!value	^ value! !!Node methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 13:48'!prevNode	^ prevNode! !!Node methodsFor: 'accessing' stamp: 'PierreMauron 10/14/2022 13:48'!value: anObject	value:= anObject! !